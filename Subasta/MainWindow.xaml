        <Button x:Name="btn_MostrarProyectos" Content="Proyectos Gen" HorizontalAlignment="Left" Margin="354,72,0,0" VerticalAlignment="Top" Width="127" Click="Btn_MostrarProyectos_Click" RenderTransformOrigin="-3.484,-3.618"/>
        <Button x:Name="btn_Ejecutar" Content="Ejecutar" HorizontalAlignment="Left" Margin="494,72,0,0" VerticalAlignment="Top" Width="127" Click="Btn_Ejecutar_Click" RenderTransformOrigin="-3.484,-3.618"/>

    </Grid>
</Window>
